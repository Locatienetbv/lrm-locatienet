{
  "name": "lrm-locatienet",
  "version": "1.0.0",
  "description": "Support for Locatienet in Leaflet Routing Machine",
  "main": "src/L.Routing.Locatienet.js",
    "scripts": {
        "watch": "watchify -t browserify-shim src/L.Routing.Locatienet.js -o dist/lrm-locatienet.js --debug --verbose",
        "dist": "browserify -t browserify-shim src/L.Routing.Locatienet.js -o dist/lrm-locatienet.js && uglifyjs dist/lrm-locatienet.js >dist/lrm-locatienet.min.js",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
  "keywords": [
    "leaflet",
    "routing",
    "locatienet"
  ],
  "author": "Remco Zut <rzu@locatienet.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/locatienetbv/lrm-locatienet.git"
  },
  "homepage": "https://github.com/locatienetbv/lrm-locatienet",
  "bugs": "https://github.com/locatienetbv/lrm-locatienet/issues",
  "dependencies": {
  },
  "browserify-shim": {
    "leaflet": "global:L"
  },
  "devDependencies": {
    "browserify": "^8.1.3",
    "browserify-shim": "^3.8.2",
    "uglify-js": "3.19.3",
    "watchify": "^4.0.0"
  }
}
